{
    "name": "webeweb/highcharts-bundle",
    "type": "symfony-bundle",
    "description": "Integrate HighchartsJS with Symfony 2 and more",
    "keywords": [
        "bundle",
        "highcharts",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/highcharts-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-json": "*",
        "symfony/framework-bundle": "^2.6|^3.0|^4.0",
        "symfony/translation": "^2.6|^3.0|^4.0",
        "twig/twig": "^1.0|^2.0",
        "webeweb/core-library": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0",
        "symfony/config": "^2.6|^3.0|^4.0",
        "symfony/expression-language": "^2.6|^3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\HighchartsBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\HighchartsBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/highcharts-bundle/issues"
    }
}
